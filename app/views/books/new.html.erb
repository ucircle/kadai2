<h1>Books</h1>
<h1>投稿一覧</h1>

<%= @books.each do |book| %>

  <p>タイトル</p>
  <span><%= book.title %></span>
  
<% end %>

<!-- form_with部分 -->


<%= form_with model: @book.new do |f| %>


<h2>New Book</h2>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>
<% end %>